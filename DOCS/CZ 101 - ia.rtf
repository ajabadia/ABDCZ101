\par # .github/workflows/build.yml
\par 
\par name: Build
\par 
\par on:
\par   push:
\par     branches: [ main, develop ]
\par     tags: [ 'v*' ]
\par   workflow_run:
\par     workflows: ["Tests"]
\par     types: [completed]
\par     branches: [main]
\par 
\par jobs:
\par   build:
\par     name: Build $\{\{ matrix.os \}\} $\{\{ matrix.arch \}\}
\par     runs-on: $\{\{ matrix.os \}\}
\par     
\par     strategy:
\par       fail-fast: false
\par       matrix:
\par         include:
\par           # macOS Intel
\par           - os: macos-latest
\par             arch: x86_64
\par             target: "Ninja Multi-Config"
\par             cpack-generator: DragNDrop
\par           
\par           # macOS Silicon (arm64)
\par           - os: macos-latest
\par             arch: arm64
\par             target: "Ninja Multi-Config"
\par             cpack-generator: DragNDrop
\par           
\par           # Windows MSVC
\par           - os: windows-latest
\par             arch: x86_64
\par             target: "Visual Studio 17 2022"
\par             cpack-generator: "NSIS;ZIP"
\par           
\par           # Windows 32-bit
\par           - os: windows-latest
\par             arch: x86
\par             target: "Visual Studio 17 2022"
\par             cpack-generator: "NSIS;ZIP"
\par           
\par           # Linux
\par           - os: ubuntu-latest
\par             arch: x86_64
\par             target: "Unix Makefiles"
\par             cpack-generator: "DEB;RPM;TGZ"
\par     
\par     steps:
\par       - uses: actions/checkout@v4
\par         with:
\par           submodules: recursive
\par       - name: Install dependencies (Ubuntu)
\par         if: runner.os == 'Linux'
\par         run: |
\par           sudo apt-get update
\par           sudo apt-get install -y \\
\par             cmake build-essential ninja-build \\
\par             libasound2-dev libjack-jackd2-dev \\
\par             libfreetype6-dev libx11-dev libxinerama-dev \\
\par             libxrandr-dev libxcursor-dev
\par       
\par       - name: Install dependencies (macOS)
\par         if: runner.os == 'macOS'
\par         run: |
\par           brew install cmake ninja
\par       
\par       - name: Install dependencies (Windows)
\par         if: runner.os == 'Windows'
\par         run: |
\par           choco install cmake ninja -y
\par       
\par       - name: Create build directory
\par         run: cmake -E make_directory $\{\{github.workspace\}\}/build
\par       
\par       - name: Configure CMake
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: |
\par           cmake .. \\
\par             -DCMAKE_BUILD_TYPE=Release \\
\par             -G "$\{\{ matrix.target \}\}" \\
\par             -DBUILD_PLUGIN=ON \\
\par             -DBUILD_HEADLESS=OFF
\par       
\par       - name: Build
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: cmake --build . --config Release -j$(nproc)
\par       
\par       - name: Create package
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: cpack -G "$\{\{ matrix.cpack-generator \}\}"
\par       
\par       - name: Upload artifacts
\par         uses: actions/upload-artifact@v3
\par         with:
\par           name: builds-$\{\{ matrix.os \}\}-$\{\{ matrix.arch \}\}
\par           path: $\{\{github.workspace\}\}/build/*.\{dmg,exe,deb,rpm,tar.gz\}
\par           retention-days: 5
