\par }{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 bash
\par #!/bin/bash
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # setup_alsa_jack.sh - Configurar ALSA y JACK en Raspberry Pi}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par 
\par set -e
\par 
\par echo "=========================================="
\par echo "ALSA & JACK Setup for Raspberry Pi"
\par echo "=========================================="
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Actualizar repositorios}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Updating package lists..."
\par sudo apt-get update
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Instalar dependencias}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Installing audio packages..."
\par sudo apt-get install -y \\
\par     alsa-utils \\
\par     alsa-tools \\
\par     libasound2-dev \\
\par     jackd2 \\
\par     libjack-jackd2-dev \\
\par     qjackctl \\
\par     pulseaudio-module-jack
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Configurar ALSA}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Configuring ALSA..."
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Detectar tarjeta de audio}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Available audio devices:"
\par aplay -l
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Crear configuraci\'f3n ALSA \'f3ptima}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par sudo tee /etc/asound.conf > /dev/null <<'EOF'
\par # ALSA configuration for low-latency audio
\par 
\par pcm.!default \{
\par     type plug
\par     slave.pcm "softvol"
\par \}
\par 
\par pcm.softvol \{
\par     type softvol
\par     slave.pcm "hw:0,0"
\par     control \{
\par         name "Master"
\par         card 0
\par     \}
\par \}
\par 
\par ctl.!default \{
\par     type hw
\par     card 0
\par \}
\par 
\par # JACK ALSA bridge
\par pcm.jack \{
\par     type jack
\par     playback_ports \{
\par         0 system:playback_1
\par         1 system:playback_2
\par     \}
\par     capture_ports \{
\par         0 system:capture_1
\par         1 system:capture_2
\par     \}
\par \}
\par 
\par ctl.jack \{
\par     type jack
\par \}
\par 
\par # Soft volume control
\par pcm.softvol \{
\par     type softvol
\par     slave.pcm "hw:0,0"
\par     control.name "Master"
\par \}
\par EOF
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Configurar permisos de audio}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Configuring audio group permissions..."
\par sudo usermod -a -G audio pi
\par sudo usermod -a -G jackuser pi
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Crear configuraci\'f3n JACK}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par mkdir -p ~/.config/jack
\par 
\par tee ~/.config/jack/conf.xml > /dev/null <<'EOF'
\par <?xml version="1.0"?>
\par <!DOCTYPE jack_conf SYSTEM "jack_conf.dtd">
\par <jack_conf>
\par   <driver name="alsa">
\par     <parameter name="device">hw:0</parameter>
\par     <parameter name="capture">hw:0</parameter>
\par     <parameter name="playback">hw:0</parameter>
\par     <parameter name="rate">44100</parameter>
\par     <parameter name="period">256</parameter>
\par     <parameter name="nperiods">2</parameter>
\par     <parameter name="inchannels">2</parameter>
\par     <parameter name="outchannels">2</parameter>
\par     <parameter name="hwmon">false</parameter>
\par   </driver>
\par   <engine>
\par     <parameter name="realtime">true</parameter>
\par     <parameter name="realtime-priority">60</parameter>
\par   </engine>
\par </jack_conf>
\par EOF
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Configurar l\'edmites de recursos para audio en tiempo real}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Configuring real-time limits..."
\par sudo tee -a /etc/security/limits.conf > /dev/null <<'EOF'
\par @audio - rtprio 80
\par @audio - memlock unlimited
\par @jackuser - rtprio 80
\par @jackuser - memlock unlimited
\par EOF
\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Optimizar kernel para latencia baja}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
