\par # .github/workflows/release.yml
\par 
\par name: Release
\par 
\par on:
\par   push:
\par     tags:
\par       - 'v[0-9]+.[0-9]+.[0-9]+*'
\par 
\par jobs:
\par   build-matrix:
\par     name: Build Release $\{\{ matrix.os \}\} $\{\{ matrix.arch \}\}
\par     runs-on: $\{\{ matrix.os \}\}
\par     
\par     strategy:
\par       matrix:
\par         include:
\par           # macOS Intel + Universal
\par           - os: macos-latest
\par             arch: universal
\par             target: "Unix Makefiles"
\par           
\par           # Windows
\par           - os: windows-latest
\par             arch: x86_64
\par             target: "Visual Studio 17 2022"
\par           
\par           # Linux (AppImage + DEB)
\par           - os: ubuntu-latest
\par             arch: x86_64
\par             target: "Unix Makefiles"
\par     
\par     outputs:
\par       version: $\{\{ steps.version.outputs.version \}\}
\par     
\par     steps:
\par       - uses: actions/checkout@v4
\par         with:
\par           submodules: recursive
\par       
\par       - name: Extract version from tag
