\par # .github/workflows/test.yml
\par 
\par name: Tests
\par 
\par on:
\par   push:
\par     branches: [ main, develop ]
\par   pull_request:
\par     branches: [ main, develop ]
\par 
\par jobs:
\par   test:
\par     runs-on: $\{\{ matrix.os \}\}
\par     
\par     strategy:
\par       matrix:
\par         os: [ubuntu-latest, macos-latest, windows-latest]
\par         include:
\par           - os: ubuntu-latest
\par             cache-path: ~/.cache/pip
\par             apt-packages: "libasound2-dev libjack-jackd2-dev libfreetype6-dev"
\par           
\par           - os: macos-latest
\par             cache-path: ~/Library/Caches/pip
\par           
\par           - os: windows-latest
\par             cache-path: ~\\AppData\\Local\\pip\\Cache
\par     
\par     steps:
\par       - uses: actions/checkout@v4
\par         with:
\par           submodules: recursive
\par       
\par       # Instalar dependencias
\par       - name: Install dependencies (Ubuntu)
\par         if: runner.os == 'Linux'
\par         run: |
\par           sudo apt-get update
\par           sudo apt-get install -y $\{\{ matrix.apt-packages \}\} cmake build-essential
\par       
\par       - name: Install dependencies (macOS)
\par         if: runner.os == 'macOS'
\par         run: |
\par           brew install cmake
\par       
\par       - name: Install dependencies (Windows)
\par         if: runner.os == 'Windows'
\par         run: |
\par           choco install cmake -y
\par       
\par       # Crear build directory
\par       - name: Create build directory
\par         run: cmake -E make_directory $\{\{github.workspace\}\}/build
\par       
\par       # Configurar CMake
\par       - name: Configure CMake
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: cmake .. -DCMAKE_BUILD_TYPE=Release -DENABLE_TESTING=ON -DENABLE_COVERAGE=OFF
\par       
\par       # Compilar
\par       - name: Build
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: cmake --build . --config Release -j$(nproc)
\par       
\par       # Ejecutar tests
\par       - name: Run tests
\par         working-directory: $\{\{github.workspace\}\}/build
\par         run: ctest --output-on-failure --parallel $(nproc)
\par       
\par       # Subir artefactos si hay fallo
\par       - name: Upload test results on failure
\par         if: failure()
\par         uses: actions/upload-artifact@v3
\par         with:
\par           name: test-results-$\{\{ matrix.os \}\}
\par           path: $\{\{github.workspace\}\}/build/Testing/
