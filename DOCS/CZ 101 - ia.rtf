\par # CMakeLists.txt
\par cmake_minimum_required(VERSION 3.21)
\par project(CZ101Emulator VERSION 1.0.0 LANGUAGES CXX)
\par 
\par set(CMAKE_CXX_STANDARD 17)
\par set(CMAKE_CXX_STANDARD_REQUIRED ON)
\par 
\par # ===== CONFIGURACI\'d3N DE PLATAFORMA =====
\par 
\par # Detectar arquitectura
\par message(STATUS "System: $\{CMAKE_SYSTEM_NAME\}")
\par message(STATUS "Processor: $\{CMAKE_SYSTEM_PROCESSOR\}")
\par 
\par # Opciones de compilaci\'f3n
\par option(ENABLE_32BIT "Compilar versi\'f3n 32-bit" OFF)
\par option(BUILD_HEADLESS "Compilar versi\'f3n headless (Raspberry Pi)" OFF)
\par option(BUILD_PLUGIN "Compilar como plugin de audio" ON)
\par option(BUILD_STANDALONE "Compilar como app standalone" OFF)
\par 
\par # Arquitectura
\par if(ENABLE_32BIT)
\par     # Forzar compilaci\'f3n 32-bit
\par     set(CMAKE_C_FLAGS "$\{CMAKE_C_FLAGS\} -m32")
\par     set(CMAKE_CXX_FLAGS "$\{CMAKE_CXX_FLAGS\} -m32")
\par     set(CMAKE_EXE_LINKER_FLAGS "$\{CMAKE_EXE_LINKER_FLAGS\} -m32")
\par     set(CMAKE_SHARED_LINKER_FLAGS "$\{CMAKE_SHARED_LINKER_FLAGS\} -m32")
\par     message(STATUS "Building 32-bit version")
\par else()
\par     message(STATUS "Building 64-bit version")
\par endif()
\par 
\par # ===== JUCE SETUP =====
\par 
\par add_subdirectory($\{CMAKE_CURRENT_SOURCE_DIR\}/JUCE)
\par 
\par juce_add_plugin(CZ101Emulator
\par     COMPANY_NAME "CZ-101"
\par     PLUGIN_MANUFACTURER_CODE Cz01
\par     PLUGIN_CODE Cz10
\par     FORMATS VST3 AU Standalone
\par     PRODUCT_NAME "CZ-101 Emulator"
\par )
\par 
\par # ===== HEADLESS MODE (Raspberry Pi) =====
\par 
\par if(BUILD_HEADLESS)
\par     # Eliminar GUI y build standalone headless
\par     juce_add_console_app(CZ101Headless
\par         PRODUCT_NAME "CZ-101 Headless"
\par     )
\par     
\par     target_compile_definitions(CZ101Headless PRIVATE
\par         BUILD_HEADLESS=1
\par         JUCE_HEADLESS_PLUGIN=1
\par     )
\par     
\par     set(BUILD_PLUGIN OFF)
\par     set(BUILD_STANDALONE OFF)
\par     
\par     message(STATUS "Building HEADLESS version for Raspberry Pi")
\par endif()
\par 
\par # ===== ARCHIVOS FUENTE =====
\par 
\par set(SOURCES
\par     # DSP Core
\par     src/dsp/PhaseDist.cpp
\par     src/dsp/SegmentEnvelope.cpp
\par     src/dsp/Voice.cpp
\par     src/dsp/VoicePool.cpp
\par     
\par     # Effects
\par     src/dsp/effects/Reverb.cpp
\par     src/dsp/effects/Chorus.cpp
\par     src/dsp/effects/Delay.cpp
\par     src/dsp/effects/FXChain.cpp
\par     
\par     # MIDI
\par     src/midi/MidiProcessor.cpp
\par     
\par     # Presets
\par     src/presets/PresetManager.cpp
\par     
\par     # Plugin
\par     src/plugin/CZ101AudioProcessor.cpp
\par )
\par 
\par # UI solo si no es headless
\par if(NOT BUILD_HEADLESS)
\par     set(SOURCES $\{SOURCES\}
\par         src/plugin/CZ101Editor.cpp
\par         src/ui/EnvelopeEditor.cpp
\par         src/ui/CustomSlider.cpp
\par     )
\par endif()
\par 
\par # ===== COMPILACI\'d3N =====
\par 
\par if(BUILD_PLUGIN)
\par     target_sources(CZ101Emulator PRIVATE $\{SOURCES\})
\par     
\par     target_compile_definitions(CZ101Emulator PUBLIC
\par         JUCE_WEB_BROWSER=0
\par         JUCE_USE_CURL=0
\par         JUCE_VST3_CAN_REPLACE_VST2=0
\par     )
\par     
\par     target_include_directories(CZ101Emulator PRIVATE
\par         $\{CMAKE_CURRENT_SOURCE_DIR\}/src
\par         $\{CMAKE_CURRENT_SOURCE_DIR\}/external/nlohmann_json/single_include
\par     )
\par     
\par     target_link_libraries(CZ101Emulator
\par         PRIVATE
\par             juce::juce_audio_processors
\par             juce::juce_dsp
\par             juce::juce_gui_extra
\par     )
\par elseif(BUILD_HEADLESS)
\par     target_sources(CZ101Headless PRIVATE $\{SOURCES\})
\par     
\par     target_compile_definitions(CZ101Headless PRIVATE
\par         JUCE_HEADLESS_PLUGIN=1
\par         JUCE_APPLICATION_NAME="CZ101Headless"
\par     )
\par     
\par     target_include_directories(CZ101Headless PRIVATE
\par         $\{CMAKE_CURRENT_SOURCE_DIR\}/src
\par         $\{CMAKE_CURRENT_SOURCE_DIR\}/external/nlohmann_json/single_include
\par     )
\par     
\par     target_link_libraries(CZ101Headless
\par         PRIVATE
\par             juce::juce_audio_processors
\par             juce::juce_dsp
\par             juce::juce_core
\par     )
\par endif()
\par 
\par # ===== OPTIMIZACIONES POR PLATAFORMA =====
\par 
\par if(CMAKE_SYSTEM_NAME MATCHES "Linux" AND CMAKE_SYSTEM_PROCESSOR MATCHES "armv7l|aarch64")
\par     # Raspberry Pi optimizations
\par     if(CMAKE_SYSTEM_PROCESSOR MATCHES "armv7l")
\par         # ARMv7 (Pi 3, Pi 4 32-bit)
\par         target_compile_options(CZ101Emulator PRIVATE
\par             -mcpu=cortex-a53
\par             -mfpu=neon-vfpv4
\par             -O3
\par             -ffast-math
\par         )
\par     elseif(CMAKE_SYSTEM_PROCESSOR MATCHES "aarch64")
\par         # ARMv8 (Pi 4 64-bit)
\par         target_compile_options(CZ101Emulator PRIVATE
\par             -mcpu=cortex-a72
\par             -O3
\par             -ffast-math
\par         )
\par     endif()
\par     
\par     message(STATUS "Building for Raspberry Pi with NEON optimizations")
\par elseif(APPLE)
\par     # macOS optimizations
\par     target_compile_options(CZ101Emulator PRIVATE
\par         -O3
\par         -march=native
\par     )
\par     message(STATUS "Building for macOS")
\par elseif(WIN32)
\par     # Windows optimizations
\par     target_compile_options(CZ101Emulator PRIVATE
\par         /O2
\par         /arch:AVX2
\par     )
\par     message(STATUS "Building for Windows")
\par endif()
\par 
\par # ===== PACKAGING =====
\par 
\par if(APPLE)
\par     set_target_properties(CZ101Emulator PROPERTIES
\par         XCODE_ATTRIBUTE_CODE_SIGN_IDENTITY ""
\par         XCODE_ATTRIBUTE_CODE_SIGNING_REQUIRED "NO"
\par     )
\par elseif(WIN32)
\par     # Windows specific
\par endif()
\par 
