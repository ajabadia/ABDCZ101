\par 
\par }{\rtlch\fcs1 \ai\af31507 \ltrch\fcs0 \i\insrsid13715973\charrsid13715973 # Establecer volumen}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid13715973\charrsid13715973 
\par echo "Setting audio levels..."
\par amixer set Master 80%
